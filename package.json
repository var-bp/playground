{
  "name": "playground",
  "version": "0.0.1",
  "private": true,
  "scripts": {
    "android:phone": "yarn run android:phone:emulator & ( sleep 30 && react-native run-android )",
    "android:phone:emulator": "/Users/bopo/Library/Android/sdk/emulator/emulator -avd Pixel_4_API_30 -wipe-data",
    "ios": "yarn run ios:emulator:erase && react-native run-ios --simulator='iPhone 12'",
    "ios:emulator:erase": "xcrun simctl erase 809A1025-E1A1-4A1E-8622-5B96C1A33712",
    "ios:emulators": "xcrun simctl list --json devices available",
    "lint": "eslint src --ext .js,.jsx",
    "lint:fix": "npm run lint -- --fix",
    "pod": "cd ios && pod install && cd ..",
    "start": "react-native start",
    "test": "jest"
  },
  "jest": {
    "preset": "react-native"
  },
  "dependencies": {
    "react": "17.0.1",
    "react-native": "0.63.4"
  },
  "devDependencies": {
    "@babel/core": "7.12.17",
    "@babel/runtime": "7.12.18",
    "babel-eslint": "10.1.0",
    "babel-jest": "26.6.3",
    "eslint": "7.20.0",
    "eslint-config-airbnb": "18.2.1",
    "eslint-config-prettier": "7.2.0",
    "eslint-plugin-import": "2.22.1",
    "eslint-plugin-jsx-a11y": "6.4.1",
    "eslint-plugin-prettier": "3.3.1",
    "eslint-plugin-react": "7.22.0",
    "eslint-plugin-react-hooks": "4.2.0",
    "eslint-plugin-react-native": "3.10.0",
    "jest": "26.6.3",
    "metro-react-native-babel-preset": "0.65.1",
    "prettier": "2.2.1",
    "react-test-renderer": "17.0.1"
  }
}
